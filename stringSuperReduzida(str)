function stringSuperReduzida(str) {
  let result = "";
  
  for (let i = 0; i < str.length; i++) {
    if (result.length > 0 && result[result.length - 1] === str[i]) {
      result = result.slice(0, result.length - 1);
    } else {
      result += str[i];
    }
  }
  
  return result.length > 0 ? result : "String Vazia";
}

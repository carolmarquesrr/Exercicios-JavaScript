function subtraiDias(data, dias) {
    // escreve aqui a função
    console.log(data)
    let horas = 24 * 60 * 60 * 1000

    let dataEmSegundos = data.valueOf()
    console.log(dataEmSegundos)

    console.log(dias)

    let day = horas * dias
    console.log(day)

    console.log(dataEmSegundos-day)
    return new Date(dataEmSegundos - day)
}

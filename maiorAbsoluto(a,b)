function maiorAbsoluto(a, b){
    
if (Math.abs(a) === Math.abs(b)) {
        return "Introduza dois valores diferentes." ;
}   else if ( Math.abs(a) > Math.abs(b)) {
        return Math.abs(a);
    } else {
         return Math.abs(b);
} 
}  console.log(maiorAbsoluto(6,5));
 

//retornar o maior valor absoluto

ASSIM TAMBEM 


function maiorAbsoluto(a, b){
    
return Math.abs(a) >= Math.abs(b) ? Math.abs(a) : Math.abs(b); 
   
}
